###_R9E_BASHRC_ENABLE_PROFILING=true
source ~/.zgen/zgen.zsh

_R9E_BASHRC_SOURCE_DEFAULT_ALIASES=false
_R9E_BASHRC_SOURCE_DEFAULT_FUNCTIONS=false
_R9E_BASHRC_ZSH_BREW_COMPLETION=false
